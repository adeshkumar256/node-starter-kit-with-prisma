{
  "name": "node-starter-kit-with-prisma",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "NODE_ENV=development && nodemon --ext js ./bin/www",
    "prisma:generate": "node prisma/schemas/index.js && prisma generate",
    "prisma:migrate": "node prisma/schemas/index.js && prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "prisma": "^5.14.0",
    "prisma-import": "^1.0.5"
  }
}
